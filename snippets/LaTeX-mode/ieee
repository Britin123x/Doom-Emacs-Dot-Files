\usetikzlibrary{automata,positioning}
\ctikzset{
  monopoles/vcc/arrow={Triangle[width=0.8*\scaledwidth,
  length=\scaledwidth]},
  monopoles/vee/arrow={Triangle[width=6pt, length=8pt]},
  diodes/scale={0.9},
  voltage/distance from node=0.5cm,
}

\sisetup{
  detect-all,          % Detect numbers and units in both math and text mode
  per-mode = symbol,     % Automatically handle units per operator like m/s
  output-complex-root = j, % Use EE notation of j for complex numbers
  complex-root-position = before-number, % Puts the "j" in front of the number in rectangular form
}

\captionsetup[table]{textfont={sc,footnotesize}, labelfont=footnotesize,
labelsep=newline}

